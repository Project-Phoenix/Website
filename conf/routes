# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.home()

#GET
GET		/student1					controllers.Application.student1()
GET		/student2					controllers.Application.student2()
GET 	/teacher					controllers.Application.teacher()
GET 	/mygroup					controllers.Application.mygroup()
GET 	/group						controllers.Application.group()
GET 	/course						controllers.Application.course()
GET 	/register					controllers.Application.register()
GET 	/logout						controllers.Login.logout()
GET		/material					controllers.Application.material()
GET		/statistics					controllers.Application.statistics()
GET		/submissions				controllers.Application.submissions()
GET		/taskpool					controllers.Application.taskpool()
GET		/create						controllers.Application.create()
GET		/exercise					controllers.Application.exercise()
GET		/solutions					controllers.Application.solutions()
GET		/doctrine					controllers.Application.doctrine()
GET 	/materialStudent			controllers.Application.materialStudent()
GET 	/sheet_create				controllers.Application.sheet_create()
GET		/AUDcourse					controllers.Application.AUDcourse()
GET		/AUDgroup1					controllers.Application.AUDgroup1()
GET		/AUDgroup2					controllers.Application.AUDgroup2()
GET		/AUDgroup3					controllers.Application.AUDgroup3()
GET		/AUDgroup4					controllers.Application.AUDgroup4()
GET		/EIcourse					controllers.Application.EIcourse()
GET		/EIgroup1					controllers.Application.EIgroup1()
GET		/EIgroup2					controllers.Application.EIgroup2()
GET		/EIgroup3					controllers.Application.EIgroup3()
GET		/EIgroup4					controllers.Application.EIgroup4()
GET		/FPcourse					controllers.Application.FPcourse()
GET		/FPgroup1					controllers.Application.FPgroup1()
GET		/FPgroup2					controllers.Application.FPgroup2()
GET		/FPgroup3					controllers.Application.FPgroup3()
GET		/FPgroup4					controllers.Application.FPgroup4()

#POST
POST 	/register					controllers.Login.register()
POST 	/login						controllers.Login.login()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
