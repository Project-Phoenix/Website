# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.home()

#GET
GET		/student1					controllers.Application.student1()
GET		/student2					controllers.Application.student2()
GET 	/teacher					controllers.Application.teacher()
GET 	/mygroup					controllers.Application.mygroup()
GET 	/group						controllers.Application.group()
GET 	/course						controllers.Application.course()
GET 	/register					controllers.Application.register()
GET 	/logout						controllers.Login.logout()
GET		/material					controllers.Application.material()
GET		/statistics					controllers.Application.statistics()
GET		/submissions				controllers.Application.submissions()
GET		/taskpool					controllers.Application.taskpool()
GET		/create						controllers.Application.create()
GET		/exercise					controllers.Application.exercise()
GET		/solutions					controllers.Application.solutions()
GET		/doctrine					controllers.Application.doctrine()
GET 	/materialStudent			controllers.Application.materialStudent()

#POST
POST 	/register					controllers.Login.register()
POST 	/login						controllers.Login.login()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
